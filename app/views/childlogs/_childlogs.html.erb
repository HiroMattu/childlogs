<ul class="media-list">
  <% childlogs.each do |childlog| %>
    <li class="media">
      <div class="media-body">
        <div>
          <%= link_to childlog.user.name, user_path(childlog.user) %> <span class="text-muted">posted at <%= childlog.created_at %></span>
        </div>
        <div>
          <p><%= childlog.content %></p>
        </div>
      </div>
    </li>
  <% end %>
  <%= paginate childlogs %>
</ul>